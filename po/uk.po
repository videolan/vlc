msgstr "аудіо"

#: modules/codec/avcodec/encoder.c:867
msgid "subpicture"
msgstr "фрагмент"

#: modules/codec/avcodec/encoder.c:880
#, c-format
msgid "VLC could not open the %4.4s %s encoder."
msgstr "VLC не зміг відкрити кодер %4.4s %s."

#: modules/codec/avcodec/vaapi.c:352
msgid "VA-API video decoder via DRM"
msgstr "Декодео відео VA-API через DRM"

#: modules/codec/avcodec/vaapi.c:357
msgid "VA-API video decoder"
msgstr "Декодер відео VA-API"

#: modules/codec/bpg.c:49
msgid "BPG image decoder"
msgstr "Декодер зображень BPG"

#: modules/codec/cc.c:51 modules/codec/zvbi.c:65
#: modules/gui/macosx/VLCSimplePrefsController.m:347 modules/spu/marq.c:108
#: modules/spu/rss.c:144 modules/gui/qt/ui/video_effects.h:1276
