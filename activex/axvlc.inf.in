; Version number and signature of INF file.
;
[version]
  signature="$CHICAGO$"
  AdvancedINF=2.0

[Add.Code]
  vlc-@VERSION@-win32.exe
  axvlc.dll=axvlc.dll

[axvlc.dll]
  FileVersion=@VERSION32@
  clsid={E23FE9C6-778E-49d4-B537-38FCDE4887D8}
  RegisterServer=no
  hook=nsiinstaller

[vlc-@VERSION@-win32.exe]
  FileVersion=@VERSION32@
  file-win32-x86=thiscab

[nsiinstaller]
  run=%EXTRACT_DIR%\vlc-@VERSION@-win32.exe

