  enableval="$enable_mmx"
   if test x$enableval = xyes; then ARCH="${ARCH} mmx";
    BUILTINS="${BUILTINS} ${ACCEL_MODULES}"; fi 
else
   if test x${host_cpu} = xi686 -o x${host_cpu} = xi586 -o x${host_cpu} = xx86 -o x${host_cpu} = xi386; then ARCH="${ARCH} mmx";
    BUILTINS="${BUILTINS} ${ACCEL_MODULES}"; fi 
